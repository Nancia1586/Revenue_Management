                                yaxis: {
                                    opposite: true
                                },
                                legend: {
                                    horizontalAlign: 'left'
                                }
                            }).render();
                        });
                    </script>
                    <!-- End Area Chart -->

                </div>
            </div>
        </div>
        <div class="col-lg-6">
            <div class="card">
                <div class="card-body">
                    

                    <!-- Donut Chart -->
                    <div id="donutChart"></div>

